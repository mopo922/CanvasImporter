    "keywords": ["canvas","laravel","l5","wordpress","blog","import"],
    "homepage": "https://github.com/mopo922/CanvasImporter",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Potter",
            "homepage": "https://github.com/mopo922"
        }
    ],
    "require": {
        "php": ">=7.0",
        "cnvs/easel": "3.3.*",
        "guzzlehttp/guzzle": "~6.0",
        "league/html-to-markdown": "^4.4"
    },
    "require-dev": {
        "laravel/framework": ">=5.0"
    },
    "autoload": {
        "psr-4" : {
            "CanvasImporter\\" : "src"
        }
    }
}